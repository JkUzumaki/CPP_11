#include<iostream>
#include<bits/stdc++.h>
int main()
{
/*
	int a = nullptr;
	std::cout << a << "\n";
*/
	nullptr_t np1, np2;
	if(np1 == np2){
		std::cout << "both equal\n";
	} else if (np1 > np2){
		std::cout << "n1 is grt\n";
	} else if( np2 > np1){
		std::cout << "n2 is grt\n";
	} else {
		std::cout << "undefined\n";
	}

	char* x  = np1;

	if(x == nullptr){
		std::cout << "x is null\n";
	} else {
		std::cout << "x is not null\n";
	}
	return 0;
}
